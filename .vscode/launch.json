{
	"version": "0.2.0",
	"configurations": [
		{
		  "name": "Current TS File",
		  "type": "node",
		  "request": "launch",
		  "args": ["${relativeFile}"],
		  "runtimeArgs": ["-r", "ts-node/register"],
		  "cwd": "${workspaceRoot}",
		  "protocol": "inspector",
		  "internalConsoleOptions": "openOnSessionStart",
		  "sourceMaps": true,
		  "outputCapture": "std"
	  }
	]
}
